{
  "name": "node-yaml",
  "version": "3.2.0",
  "description": "A wrapper for js-yaml parser",
  "repository": "octet-stream/node-yaml",
  "keywords": [
    "wrapper",
    "yaml",
    "parser",
    "serializer"
  ],
  "main": "lib/node-yaml",
  "author": "Nick K.",
  "license": "MIT",
  "scripts": {
    "test": "ava",
    "staged": "lint-staged",
    "eslint": "eslint lib test",
    "coverage": "nyc npm run test",
    "report": "npm run coverage && nyc report --reporter=html",
    "report:ci": "npm run coverage && nyc report --reporter=lcov",
    "ci": "npm run report:ci && codecov"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/fixture/**"
    ]
  },
  "dependencies": {
    "co": "4.6.0",
    "junk": "3.0.0",
    "promise-fs": "2.1.0"
  },
  "peerDependencies": {
    "js-yaml": "^3.13.x"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "3.0.0",
    "ava": "1.4.1",
    "codecov": "3.2.0",
    "coffee-script": "1.12.7",
    "coffeelint": "1.16.0",
    "eslint": "5.15.3",
    "eslint-plugin-ava": "6.0.0",
    "husky": "1.3.1",
    "js-yaml": "3.13.x",
    "lint-staged": "8.1.5",
    "lodash.isplainobject": "4.0.6",
    "nyc": "13.3.0",
    "proxyquire": "2.1.0",
    "sinon": "7.3.1"
  }
}

{
  "name": "node-yaml",
  "version": "3.2.0",
  "description": "A wrapper for js-yaml parser",
  "repository": "octet-stream/node-yaml",
  "keywords": [
    "wrapper",
    "yaml",
    "parser",
    "serializer"
  ],
  "main": "lib/node-yaml",
  "author": "Nick K.",
  "license": "MIT",
  "scripts": {
    "test": "ava",
    "staged": "lint-staged",
    "eslint": "eslint lib test readme.md",
    "coverage": "nyc npm run test",
    "report": "npm run coverage && nyc report --reporter=html",
    "report:ci": "npm run coverage && nyc report --reporter=lcov",
    "ci": "npm run report:ci && codecov"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/fixture/**"
    ]
  },
  "dependencies": {
    "co": "4.6.0",
    "junk": "3.1.0",
    "promise-fs": "2.1.0"
  },
  "peerDependencies": {
    "js-yaml": "^3.13.x"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "3.0.0",
    "ava": "2.1.0",
    "codecov": "3.5.0",
    "eslint": "5.16.0",
    "eslint-plugin-ava": "7.1.0",
    "eslint-plugin-markdown": "1.0.0",
    "husky": "2.4.1",
    "js-yaml": "3.13.1",
    "lint-staged": "8.2.1",
    "nyc": "14.1.1",
    "proxyquire": "2.1.0",
    "sinon": "7.3.2"
  }
}

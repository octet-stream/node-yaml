{
  "name": "node-yaml",
  "version": "3.2.0",
  "description": "A wrapper for js-yaml parser",
  "keywords": [
    "wrapper",
    "yaml",
    "parser",
    "serializer"
  ],
  "main": "main.js",
  "author": "Nick K.",
  "license": "MIT",
  "scripts": {
    "lint": "coffeelint main.coffee",
    "make": "coffee --no-header -b -c main.coffee test.coffee",
    "m": "npm run make",
    "watch": "coffee --no-header -b -cw main.coffee test.coffee",
    "w": "npm run watch",
    "make:ci": "coffee --no-header -b -cm main.coffee test.coffee",
    "test": "ava",
    "test:dev": "ava -w",
    "t:d": "npm run test dev",
    "cover": "nyc npm run test",
    "report": "npm run make:ci && npm run cover && nyc report --reporter=html",
    "report:ci": "npm run make:ci && npm run cover && nyc report --reporter=lcov",
    "ci": "npm run report:ci && codecov",
    "precommit": "npm run lint",
    "prepush": "npm run report"
  },
  "ava": {
    "verbose": true,
    "files": [
      "./test.js",
      "!test/**/.js"
    ]
  },
  "dependencies": {
    "co": "4.6.0",
    "js-yaml": "^3.13.x",
    "junk": "3.0.0",
    "promise-fs": "2.1.0"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "codecov": "3.2.0",
    "coffee-script": "1.12.7",
    "coffeelint": "1.16.0",
    "husky": "0.14.3",
    "lodash.isplainobject": "4.0.6",
    "nyc": "13.3.0",
    "proxyquire": "2.1.0",
    "sinon": "7.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octet-stream/node-yaml.git"
  },
  "bugs": {
    "url": "https://github.com/octet-stream/node-yaml/issues"
  }
}
